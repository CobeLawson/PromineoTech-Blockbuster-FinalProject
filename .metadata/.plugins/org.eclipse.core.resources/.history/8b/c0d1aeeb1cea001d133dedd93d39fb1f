-- customers table --
CREATE TABLE customers (
	customer_pk int unsigned NOT NULL AUTO_INCREMENT, 
	customer_name varchar(50) NOT NULL,
	customer_age int NOT NULL,
	customer_phone varchar(25) NOT NULL,
	PRIMARY KEY (customer_pk)
);

